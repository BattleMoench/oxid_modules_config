#!/usr/bin/env php
<?php
// application.php

require __DIR__.'/../../../../bootstrap.php';

use Symfony\Component\Console\Application;
use Oxps\ModulesConfig\Commands\OxpsConfigImportCommand;
use Oxps\ModulesConfig\Commands\OxpsConfigExportCommand;

$application = new Application();

$application->add(new OxpsConfigExportCommand());

$application->add(new OxpsConfigImportCommand());
    
    /** @var Application $application */
    try {
        $application->run();
    } catch (Exception $e) {
    }